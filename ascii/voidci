#!/bin/bash

# Void
col1=$(tput setaf 10) #lightgreen
col2=$(tput setaf 8) #black
rst=$(tput sgr0)
s=$(echo "\\")

cat <<EOF
$col1
        ,  -----  ,                   
     ,'     ___    ".   $col2 _ _$col1
 ___    _ ,___ :__ __   $col2| (_)_ __  _   _ _  __$col1
 \  \  /// _  \/ //_  \ $col2| | | '_ \| | | | \/ /$col1
  \  \/// /_/// ///_/ / $col2| | | | | | |_| |>  <$col1
   \__/ \___//_//___./  $col2|_|_|_| |_|\__,_|_/\_$s$col1
    ^.   '--- "    *
      "  _____ - '
$rst
EOF
      
#        ,  -----  ,                   
#     ,'     ___    ".    _ _
# ___    _ ,___ :__ __   | (_)_ __  _   _ _  __
# \  \  /// _  \/ //_  \ | | | '_ \| | | | \/ /
#  \  \/// /_/// ///_/ / | | | | | | |_| |>  <
#   \__/ \___//_//___./  |_|_|_| |_|\__,_|_/\_\
#    ^.   '--- "    *
#      "  _____ - '

#!/bin/bash

# endeavouros
col1=$(tput setaf 13)	#magenta
col2=$(tput setaf 11)	#lightred
col3=$(tput setaf 8)	#lightblack
rst=$(tput sgr0)
s=$(echo "\\")

cat <<EOF

$col1         ,---.,      $col3 ____  _   _  ____   ____   ___   _   _   ___   _   _  ____   $col1 ___   ___
$col2       .$col1/     '*.   $col3 |  __|| \ | ||  _ \ |  __| /   \ | | | | / _ \ | | | ||  _ \ $col1 / _ \ /  _ $s
$col2     .'$col1/         ^: $col3 | |__ |  \| || | \ || |__ |     || | | || / \ || | | || |_)|$col1 | / \ || ||_|
$col2    / $col1/           '.$col3 |  __||     || | | ||  __||  _  || |_| || | | || | | ||  _ /$col1 | | | | \  \   
$col2  /  $col1.'           ./$col3 | |__ | |\  || |_/ || |__ | | | ||     || \_/ || |_| || || |$col1 | \_/ ||'|; |
$col2 "--$col1/            ,./$col3 |____||_| \_||____/ |____||_| |_| \___/  \___/  \__ / |_||_| $col1 \___/ \____/
$col1   "..__,../.-- '
$rst

EOF
      


#         ,---.,       ____  _   _  ____   ____   ___   _   _   ___   _   _  ____    ___   ___
#       ./     '*.    |  __|| \ | ||  _ \ |  __| /   \ | | | | / _ \ | | | ||  _ \  / _ \ /  _ \
#     .'/         ^:  | |__ |  \| || | \ || |__ |     || | | || / \ || | | || |_)| | / \ || ||_|
#    / /           '. |  __||     || | | ||  __||  _  || |_| || | | || | | ||  _ / | | | | \  \   
#  /  .'           ./ | |__ | |\  || |_/ || |__ | | | ||     || \_/ || |_| || || | | \_/ ||'|; |
# "--/            ,./ |____||_| \_||____/ |____||_| |_| \___/  \___/  \__ / |_||_|  \___/ \____/
#   "..__,../.-- '
